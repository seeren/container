{
    "name": "seeren/container",
    "description": "Resolve and cache dependencies",
    "keywords": ["psr-11", "container", "dependency-injection", "resolver"],
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Cyril Ichti",
        "email": "consultant@seeren.fr",
        "homepage" : "http://www.seeren.fr"
      }
    ],
    "minimum-stability": "dev",
    "require": {
      "php": ">=7.0.1",
      "psr/container": "1.0.x-dev"
    },
    "autoload": {
      "psr-4": {
		"Seeren\\Container\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
		"Seeren\\Container\\Test\\": "test/"
      }
    },
    "require-dev": {
        "phpunit/phpunit": "6.1.*",
        "satooshi/php-coveralls": "dev-master"
    }
}