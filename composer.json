{
  "name": "seeren/container",
  "description": "Resolve, contain and inject dependencies",
  "keywords": [
    "container",
    "service",
    "psr-11"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Cyril Ichti",
      "email": "consultant@seeren.fr",
      "homepage": "https://github.com/seeren/container"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4.0",
    "psr/container": "^1.0.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Seeren\\Container\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seeren\\Container\\Test\\": "test/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "9.4.0",
    "php-coveralls/php-coveralls": "v2.4.1"
  }
}
